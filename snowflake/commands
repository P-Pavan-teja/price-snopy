SELECT  *  FROM  TABLE ( INFER_SCHEMA ( LOCATION  => '@ADMIN_DB.UTIL_SCH.ADMIN_S3_STAGE/ext_stg/',  FILE_FORMAT  => '"DEMO_DB"."UTIL_SCH".CSV_FORMAT_COMMA'));

-- Set tag before COPY commands
ALTER SESSION SET QUERY_TAG = 'LOAD_PROC_ABC_1';

-- Your ASYNC COPY commands here
ASYNC ( ... COPY INTO ... );
ASYNC ( ... COPY INTO ... );
AWAIT ALL;

-- Unset the tag after finishing
ALTER SESSION UNSET QUERY_TAG;

SELECT QUERY_ID, QUERY_TEXT, START_TIME
FROM TABLE(INFORMATION_SCHEMA.QUERY_HISTORY(
    RESULT_LIMIT => 1000
))
WHERE QUERY_TAG = 'LOAD_PROC_ABC_1'
ORDER BY START_TIME DESC;
